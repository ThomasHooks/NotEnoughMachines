{
    "multipart": [
		{   "when": { "facing": "down" },
            "apply": { "model": "notenoughtmachines:block/chute", "x": 90, "uvlock": false }
        },
		{   "when": { "facing": "up" },
            "apply": { "model": "notenoughtmachines:block/chute", "x": -90, "uvlock": false }
        },
		{   "when": { "facing": "north" },
            "apply": { "model": "notenoughtmachines:block/chute", "y": 0, "uvlock": false }
        },
		{   "when": { "facing": "east" },
            "apply": { "model": "notenoughtmachines:block/chute", "y": 90, "uvlock": false }
        },
		{   "when": { "facing": "south" },
            "apply": { "model": "notenoughtmachines:block/chute", "y": 180, "uvlock": false }
        },
		{   "when": { "facing": "west" },
            "apply": { "model": "notenoughtmachines:block/chute", "y": -90, "uvlock": false }
        },
	
	
        {   "when": { "north": "true" },
            "apply": { "model": "notenoughtmachines:block/closedchute_side", "y": 180, "uvlock": false }
        },
        {   "when": { "east": "true" },
            "apply": { "model": "notenoughtmachines:block/closedchute_side", "y": -90, "uvlock": false }
        },
        {   "when": { "south": "true" },
            "apply": { "model": "notenoughtmachines:block/closedchute_side", "y": 0, "uvlock": false }
        },
        {   "when": { "west": "true" },
            "apply": { "model": "notenoughtmachines:block/closedchute_side", "y": 90, "uvlock": false }
        },
        {   "when": { "up": "true" },
            "apply": { "model": "notenoughtmachines:block/closedchute_side", "x": 90, "uvlock": false }
        },
        {   "when": { "down": "true" },
            "apply": { "model": "notenoughtmachines:block/closedchute_side", "x": -90, "uvlock": false }
        }
    ]
}