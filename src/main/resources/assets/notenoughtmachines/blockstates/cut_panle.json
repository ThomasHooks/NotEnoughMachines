{
    "multipart": [
		{   "when": { "facing": "down" },
            "apply": { "model": "notenoughtmachines:block/cut_panle", "x": 0, "uvlock": false }
        },
		{   "when": { "facing": "up" },
            "apply": { "model": "notenoughtmachines:block/cut_panle", "x": 180, "uvlock": false }
        },
		{   "when": { "facing": "north" },
            "apply": { "model": "notenoughtmachines:block/cut_panle", "x": -90, "uvlock": false }
        },
		{   "when": { "facing": "east" },
            "apply": { "model": "notenoughtmachines:block/cut_panle", "x": 90, "y": -90, "uvlock": false }
        },
		{   "when": { "facing": "south" },
            "apply": { "model": "notenoughtmachines:block/cut_panle", "x": 90, "uvlock": false }
        },
		{   "when": { "facing": "west" },
            "apply": { "model": "notenoughtmachines:block/cut_panle", "x": 90, "y": 90, "uvlock": false }
        }
    ]
}