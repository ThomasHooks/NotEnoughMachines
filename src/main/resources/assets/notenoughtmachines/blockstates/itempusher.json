{
    "multipart": [
		{   "when": { "facing": "down", "enabled": "true" },
            "apply": { "model": "notenoughtmachines:block/itempusher_unlocked", "x": 90, "uvlock": false }
        },
		{   "when": { "facing": "up", "enabled": "true" },
            "apply": { "model": "notenoughtmachines:block/itempusher_unlocked", "x": -90, "uvlock": false }
        },
		{   "when": { "facing": "north", "enabled": "true" },
            "apply": { "model": "notenoughtmachines:block/itempusher_unlocked", "y": 0, "uvlock": false }
        },
		{   "when": { "facing": "east", "enabled": "true" },
            "apply": { "model": "notenoughtmachines:block/itempusher_unlocked", "y": 90, "uvlock": false }
        },
		{   "when": { "facing": "south", "enabled": "true" },
            "apply": { "model": "notenoughtmachines:block/itempusher_unlocked", "y": 180, "uvlock": false }
        },
		{   "when": { "facing": "west", "enabled": "true" },
            "apply": { "model": "notenoughtmachines:block/itempusher_unlocked", "y": -90, "uvlock": false }
        },
		{   "when": { "facing": "down", "enabled": "false" },
            "apply": { "model": "notenoughtmachines:block/itempusher_locked", "x": 90, "uvlock": false }
        },
		{   "when": { "facing": "up", "enabled": "false" },
            "apply": { "model": "notenoughtmachines:block/itempusher_locked", "x": -90, "uvlock": false }
        },
		{   "when": { "facing": "north", "enabled": "false" },
            "apply": { "model": "notenoughtmachines:block/itempusher_locked", "y": 0, "uvlock": false }
        },
		{   "when": { "facing": "east", "enabled": "false" },
            "apply": { "model": "notenoughtmachines:block/itempusher_locked", "y": 90, "uvlock": false }
        },
		{   "when": { "facing": "south", "enabled": "false" },
            "apply": { "model": "notenoughtmachines:block/itempusher_locked", "y": 180, "uvlock": false }
        },
		{   "when": { "facing": "west", "enabled": "false" },
            "apply": { "model": "notenoughtmachines:block/itempusher_locked", "y": -90, "uvlock": false }
        },
	
	
        {   "when": { "north": "true" },
            "apply": { "model": "notenoughtmachines:block/closedchute_side", "y": 180, "uvlock": false }
        },
        {   "when": { "east": "true" },
            "apply": { "model": "notenoughtmachines:block/closedchute_side", "y": -90, "uvlock": false }
        },
        {   "when": { "south": "true" },
            "apply": { "model": "notenoughtmachines:block/closedchute_side", "y": 0, "uvlock": false }
        },
        {   "when": { "west": "true" },
            "apply": { "model": "notenoughtmachines:block/closedchute_side", "y": 90, "uvlock": false }
        },
        {   "when": { "up": "true" },
            "apply": { "model": "notenoughtmachines:block/closedchute_side", "x": 90, "uvlock": false }
        },
        {   "when": { "down": "true" },
            "apply": { "model": "notenoughtmachines:block/closedchute_side", "x": -90, "uvlock": false }
        }
    ]
}